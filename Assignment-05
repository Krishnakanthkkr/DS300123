# class Point:    
#     def __init__(self, x, y, z):
#         self.x = x
#         self.y = y
#         self.z = z
    
#     def sqSum(self):
#         return self.x**2 + self.y**2 + self.z**2
# p = Point(2,4,6)
# print(p.sqSum()) # Output: 56


class Calculator:
    def __init__(self):
        self.num1 = int(input("please enter num1:"))
        self.num2 = int(input("please enter num2:"))
    
    def add(self):
        return self.num1 + self.num2
    
    def subtract(self):
        return self.num2 - self.num1
    
    def multiply(self):
        return self.num1 * self.num2
    
    def divide(self):
        return self.num2 / self.num1
obj = Calculator()
print(obj.add())      
print(obj.subtract()) 
print(obj.multiply()) 
print(obj.divide())  

